.content{
  font-family: "Microsoft YaHei";
}

.menu{
  display: flex;
  flex-direction: row;
  width: 100%;
  border-bottom: 1px solid #f2f2f2;
}

.menu view{
  margin: 0 auto;
}

.select{
  font-size: 35.15625rpx;
  width: 33%;
  text-align: center;
  height: 105.46875rpx;
  line-height:105.46875rpx;
  border-bottom: 5rpx solid rgba(24, 24, 22, 0.897);
}

.default{
  width: 33%;
  font-size: 35.15625rpx;
  text-align: center;
  height: 105.46875rpx;
  line-height: 105.46875rpx;
}

.name{
  width: 80%;
  color: #333030e5;
}

.fname{
  margin: 20rpx 50rpx 20rpx 20rpx;
  font-weight: bold;
}

.time{
  margin: 10rpx 50rpx 20rpx 20rpx;
}

.status{
  margin: 10rpx 50rpx 20rpx 20rpx;
}

.count{
  width: 20%;
  font-size: 40rpx;
  display: flex;
  flex-direction: column;
  /* margin: 40px 20px 20px; */
}

.item{
  border: 2.5rpx solid #333030d8;
  width: 96%;
  height: 250rpx;
  margin:  0 auto;
  border-radius: 15rpx;
  margin-top: 20rpx;
}

.nameInfo{
  display: flex;
  flex-direction: row;
  padding: 12rpx;
  height: 200rpx;
}
